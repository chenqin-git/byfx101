<%= simple_form_for [:admin, @quotation] do |f| %>
  <div class="form-group">
    <%= f.input :price, as: :integer, input_html: { class: "form-control"} %>
    <p><%= f.association :product, collection: Product.all, priority: 1, input_html: { class: "form-control"} %></p>
    <p><%= f.association :agent_rank, collection: AgentRank.all, priority: 1, input_html: { class: "form-control"} %></p>
  </div>
  <%= f.submit "提交", class: "btn btn-primary", data: { disable_with: "提交中..." } %>
<% end %>
