<%= simple_form_for @agent_rank do |f| %>
  <div class="form-group">
    <%= f.input :name, input_html: { class: "form-control"} %>
    <%= f.input :rank, input_html: { class: "form-control"} %>
    <%= f.input :remark, input_html: { class: "form-control"} %>
  </div>
  <%= f.submit "提交", class: "btn btn-primary", data: { disable_with: "提交中..." } %>
<% end %>
